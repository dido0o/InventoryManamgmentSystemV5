@model IEnumerable<InventoryMnamgmentSystemV5.Models.UsersItems>

@{
    ViewBag.Title = "Report";
}

<h2 style="margin:10px">تقرير العناصر والمستخدمين</h2>


<div>
    <table class="table">
        <tr>
            <th>
                <h3>الرقم التسلسلي</h3>
            </th>
            <th>
                <h3>اسم المستخدم</h3>
            </th>
            <th>
                <h3>حالة العنصر</h3>
            </th>
            <th>
                <h3>الوقت والتاريخ</h3>
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Items.ISerialNum)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.VUsers.UserName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.IStatus.StatusName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.currentDateTime)
                </td>
            </tr>
        }

    </table>
</div>
